<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expmanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wallet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wallet"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExpManager">
        <activity android:name=".News.News"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityReportTimeline"
            android:label="@string/Timeline"
            android:theme="@style/Theme.ExpManager.NoActionBar" />
        <activity
            android:name=".ActivityReportCategory"
            android:label="@string/Categories"
            android:theme="@style/Theme.ExpManager.NoActionBar" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ResetActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".StartActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>